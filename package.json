{
  "name": "movie",
  "productName": "movie",
  "version": "1.0.0",
  "description": "Electron app with electron-forge, svelte, webpack and typescript!",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "DylanXie123",
    "email": "dylanxie123@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "learn_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' data: https:; script-src 'self' 'unsafe-eval' 'unsafe-inline' data:",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.ts",
                  "preload": {
                    "js": "./src/preload/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@tsconfig/svelte": "^2.0.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/bootstrap": "^5.1.6",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "dotenv-webpack": "^7.0.3",
    "electron": "^13.6.1",
    "node-loader": "^2.0.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.0.0",
    "svelte": "^3.43.1",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.8",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.4",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "path-browserify": "^1.0.1",
    "simplebar": "^5.3.6",
    "svelte-spa-router": "^3.2.0"
  }
}
